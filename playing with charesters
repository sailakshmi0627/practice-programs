#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    char ch;
    char s[100];
    char sen[10000];
    scanf("%c",&ch);
    scanf("%s\n",s);
    scanf("%[^\n]%*c",sen);
    printf("%c\n%s\n%s",ch,s,sen);
    return 0;
    


    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
